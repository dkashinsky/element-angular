{
  "name": "@fsdk/element-angular",
  "version": "0.7.6",
  "description": "element ui for angular",
  "private": true,
  "scripts": {
    "start": "ng build element-angular && ng serve -o",
    "build": "ng build",
    "build:publish": "ng build && gulp assets",
    "ex:publish": "node build/publish.js",
    "i18n": "./node_modules/.bin/ng-xi18n -p ./ex --i18nFormat=xlf --locale=en-US",
    "elements:upload": "node build/bin/elements-upload.locale",
    "elements:update": "node build/bin/elements-update.locale",
    "docs:upload": "node build/bin/docs-upload.locale",
    "docs:update": "node build/bin/docs-update.locale",
    "login": "node build/bin/login.locale",
    "commit": "node_modules/commitizen/bin/commitizen init cz-conventional-changelog --save --save-exact",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dkashinsky/element-angular.git"
  },
  "keywords": [
    "element",
    "angular",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/dkashinsky/element-angular/issues"
  },
  "author": "Dmitry Kashinsky",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "^6.2.9",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@angular/platform-server": "^6.1.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/highlight.js": "^9.1.9",
    "@types/node": "~8.9.4",
    "@types/webpack": "^3.0.9",
    "app-root-path": "^2.0.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "element-ui": "2.0.7",
    "gh-pages": "^1.0.0",
    "gulp": "^4.0.2",
    "highlight.js": "^9.12.0",
    "husky": "^0.14.3",
    "i": "^0.3.5",
    "jsdom": "^11.2.0",
    "ng-packagr": "^4.1.0",
    "ora": "^1.3.0",
    "request-promise-native": "^1.0.5",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "^5.5.0",
    "typescript": "^2.9.2",
    "xml2js": "^0.4.19"
  },
  "peerDependencies": {},
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
