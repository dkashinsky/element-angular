{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@fsdk/element-angular": [
        "dist/element-angular"
      ],
      "@fsdk/element-angular/*": [
        "dist/element-angular/*"
      ]
    }
  }
}